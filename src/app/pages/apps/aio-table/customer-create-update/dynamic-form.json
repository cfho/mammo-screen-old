[
  {
    "className": "section-label",
    "template": "<hr /><div><strong>個案基本資料</strong></div>"
  },
  {
    "fieldGroupClassName": "display-flex",
    "fieldGroup": [
      {
        "key": "_003name",
        "type": "input",
        "className": "flex-3",
        "templateOptions": {
          "label": "姓名",
          "required": true
        }
      },
      {
        "key": "_037hormone_age",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "type": "number",
          "label": "Hormon age",
          "min": 18
        },
        "validation": {
          "messages": {
            "min": "Sorry, you have to be older than 18"
          }
        }
      }
    ]
  },
  {
    "className": "section-label",
    "template": "<hr /><div><strong>疾病史-有無得過下列疾病?</strong></div>"
  },
  {
    "fieldGroupClassName": "display-flex",
    "fieldGroup": [
      {
        "key": "_020past_hx",
        "type": "radio",
        "className": "flex-1",
        "templateOptions": {
          "label": "有",
          "placeholder": "Placeholder",
          "description": "Description",
          "required": true,
          "options": [
            { "value": "0", "label": "無" },
            { "value": "1", "label": "乳房良性相關疾病" },
            { "value": "2", "label": "乳癌" },
            { "value": "3", "label": "其他癌症" }
          ]
        }
      },
      {
        "key": "_021cancer_hx",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "其他癌症描述",
          "max": 25
        },
        "validation": {
          "messages": {
            "min": "Sorry, you have to be older than 18"
          }
        }
      }
    ]
  },
  {
    "className": "section-label",
