    "fieldGroupClassName": "display-flex",
    "fieldGroup": [
      {
        "key": "_033production_number",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "生育次數",
          "type": "number",
          "required": true
        }
      },
      {
        "key": "_034isBreastFeeding",
        "type": "checkbox",
        "className": "flex-1",
        "templateOptions": {
          "label": "有無哺餵母乳",
          "required": true
        }
      },
      {
        "key": "_035production_age",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "第一胎生產年齡",
          "type": "number",
          "max": 99
        }
      }
    ]
  },
  {
    "className": "section-label",
