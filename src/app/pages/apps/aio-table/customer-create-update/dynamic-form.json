    "fieldGroupClassName": "display-flex",
    "fieldGroup": [
      {
        "key": "_039isContraceptive",
        "type": "checkbox",
        "className": "flex-1",
        "templateOptions": {
          "label": "是否服用避孕藥？",
          "required": true
        }
      },
      {
        "key": "_040contraceptive_age",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "幾歲開始(避)",
          "type": "number",
          "max": 99
        }
      },
      {
        "key": "_041contracepative_period",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "服用幾年(避)",
          "type": "number"
        }
      }
    ]
  },
  {
    "className": "section-label",
    "template": "<hr /><div><strong>腫塊</strong></div>"
