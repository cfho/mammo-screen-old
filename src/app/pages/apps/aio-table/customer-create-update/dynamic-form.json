      {
        "key": "_031menopause_causes_2",
        "type": "checkbox",
        "className": "flex-3",
        "templateOptions": {
          "label": "自然停經"
        }
      },
      {
        "key": "_031menopause_causes_3",
        "type": "checkbox",
        "className": "flex-3",
        "templateOptions": {
          "label": "子宮切除"
        }
      },
      {
        "key": "_031menopause_causes_4",
        "type": "checkbox",
        "className": "flex-3",
        "templateOptions": {
          "label": "卵巢切除"
        }
      },
      {
        "key": "_031menopause_causes_5",
        "type": "checkbox",
        "className": "flex-3",
        "templateOptions": {
          "label": "其他"
        }
      },
      {
        "key": "_032menopause_other",
        "type": "input",
        "className": "flex-1",
        "templateOptions": {
          "label": "停經原因其他敘述",
          "max": 25
        }
      }
    ]
  },
  {
    "className": "section-label",
